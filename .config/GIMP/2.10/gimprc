(tile-cache-size 512M)
(undo-levels 10)
(undo-size 512M)
(color-management
    (mode display)
    (display-profile-from-gdk no)
    (display-rendering-intent relative-colorimetric)
    (display-use-black-point-compensation yes)
    (display-optimize no)
    (simulation-rendering-intent perceptual)
    (simulation-use-black-point-compensation no)
    (simulation-optimize no)
    (simulation-gamut-check no)
    (out-of-gamut-color (color-rgb 1 0 1))
    (display-module "CdisplayLcms"))
(import-raw-plug-in "${gimp_plug_in_dir}/plug-ins/file-raw-placeholder/file-raw-placeholder")
(debug-policy never)
(monitor-xresolution 157)
(monitor-yresolution 161)
(filter-tool-show-color-options yes)
(theme "Nord Dark")
(fill-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10))
(stroke-options
    (style solid)
    (antialias yes)
    (feather no)
    (feather-radius 10)
    (method line)
    (width 6)
    (unit pixels)
    (cap-style butt)
    (join-style miter)
    (miter-limit 10)
    (dash-offset 0)
    (dash-info 0)
    (emulate-brush-dynamics no))


